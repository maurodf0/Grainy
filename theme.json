    "styles": {
        "blocks": {
            "core/button": {
                "color": {
                    "background": "var(--wp--preset--color--primary)",
                    "text": "var(--wp--preset--color--text)"
                }
            },
            "core/comment-date": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/group": {
                "spacing": {
                    "padding": {
                        "bottom": "var(--wp--preset--spacing--40)",
                        "left": "var(--wp--preset--spacing--40)",
                        "right": "var(--wp--preset--spacing--40)",
                        "top": "var(--wp--preset--spacing--40)"
                    }
                }
            },
            "core/heading": {
                "color": {
                    "text": "var(--wp--preset--color--text)"
                }
            },
            "core/post-title": {
                "elements": {
                    "link": {
                        ":hover": {
                            "typography": {
                                "textDecoration": "underline"
                            }
                        },
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/query-title": {
                "color": {
                    "text": "var(--wp--preset--color--primary)"
                },
                "typography": {
                    "textDecoration": "none"
                }
            },
            "core/site-title": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            }
        },
        "color": {
            "background": "var(--wp--preset--color--background)",
            "text": "var(--wp--preset--color--text)"
        },
        "css": "body {background: url(https://grainy-gradients.vercel.app/noise.svg) repeat, var(--wp--preset--color--background);background-blend-mode: overlay;}",
        "elements": {
            "button": {
                "color": {
                    "background": "var(--wp--preset--color--primary)"
                }
            },
            "caption": {
                "color": {
                    "text": "var(--wp--preset--color--primary)"
                }
            },
            "heading": {
                "color": {
                    "text": "var(--wp--preset--color--text)"
                }
            },
            "link": {
                ":hover": {
                    "color": {
                        "text": "var(--wp--preset--color--secondary)"
                    }
                },
                "color": {
                    "text": "var(--wp--preset--color--primary)"
                },
                "typography": {
                    "textDecoration": "underline"
                }
            }
        },
        "spacing": {
            "padding": {
                "bottom": "0px",
                "left": "0px",
                "right": "0px",
                "top": "0px"
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--bitter)",
            "fontSize": "var(--wp--preset--font-size--normal)",
            "fontStyle": "normal",
            "fontWeight": "400",
            "lineHeight": "1.6"
        }
